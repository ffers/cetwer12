server {
        if ($host = asxcrm.fun) {
            return 301 https://$host$request_uri;
        }
        listen 80;
        server_name asxcrm.fun;
        return 404; # managed by Certbot

location / {
        proxy_pass http://127.0.0.1:8000;
        proxy_set_header Host $host;
        proxy_set_header X-Real-IP $remote_addr;
    }

    location /static {
        alias /root/1app/static; # Наприклад, /var/www/myapp/static
    }

    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/asxcrm.fun/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/asxcrm.fun/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot
}


    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/asxcrm.fun/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/asxcrm.fun/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot


        server_name asxcrm.fun;

        if ($host = asxcrm.fun) {
            return 301 https://$host$request_uri;
        }


server
{ listen 80;
server_name ваш_домен;
location / { proxy_pass http://127.0.0.1:8000;
